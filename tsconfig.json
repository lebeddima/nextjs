{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src",
    "paths": {
      "@/utils/*": ["utils/*"],
      "@/styles/*": ["styles/*"],
      "@/store": ["store"],
      "@/store/*": ["store/*"],
      "@/types/*": ["types/*"],
      "@/icons/*": ["assets/icons/*"],
      "@/components/*": ["components/*"],
      "@/containers/*": ["containers/*"],
      "@/images/*": ["assets/images/*"],
      "@/hooks/*": ["hooks/*"],
      "@/features/*": ["features/*"],
      "@/requests/*": ["api/requests/*"],
      "@/browserApi/*": ["api/browser-api/*"],
      "@/constants/*": ["constants/*"],
      "@/routes": ["constants/routes"],
      "@/permissions/*": ["permissions/*"],
      "@/layouts/*": ["layouts/*"],
      "@/api/*": ["api/*"],
      "@/temp2/*": ["temp2/*"],
      "@/temp/*": ["temp/*"],
      "@/stylesQuan2um/*": ["temp/styles/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
